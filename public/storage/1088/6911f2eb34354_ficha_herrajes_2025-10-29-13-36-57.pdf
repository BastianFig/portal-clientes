22ESTADO\u0022:\u0022\u0022,\u0022scale\u0022:true,\u0022tipo\u0022:\u0022derivado\u0022}],\u0022encontrado\u0022:true},{\u0022label\u0022:\u0022Gabinete 50\\u00d750\\u00d760 cm\u0022,\u0022product\u0022:\u0022gabinete\u0022,\u0022type\u0022:\u0022medida\u0022,\u0022variant\u0022:\u002250-50-60\u0022,\u0022muebles\u0022:1,\u0022dim\u0022:{\u0022ancho\u0022:\u002250\u0022,\u0022fondo\u0022:\u002250\u0022,\u0022alto\u0022:\u002260\u0022},\u0022dimBuscadas\u0022:{\u0022ancho\u0022:50,\u0022fondo\u0022:50,\u0022alto\u0022:60},\u0022con_cubierta\u0022:false,\u0022rows\u0022:[{\u0022DETALLE\u0022:\u0022MINIFIX\u0022,\u0022CANTIDAD\u0022:12,\u0022ESTADO\u0022:\u0022\u0022,\u0022scale\u0022:true,\u0022tipo\u0022:\u0022base\u0022},{\u0022DETALLE\u0022:\u0022TARUGO DE MADERA\u0022,\u0022CANTIDAD\u0022:12,\u0022ESTADO\u0022:\u0022\u0022,\u0022scale\u0022:true,\u0022tipo\u0022:\u0022base\u0022},{\u0022DETALLE\u0022:\u0022PATINES\u0022,\u0022CANTIDAD\u0022:4,\u0022ESTADO\u0022:\u0022\u0022,\u0022scale\u0022:true,\u0022tipo\u0022:\u0022base\u0022},{\u0022DETALLE\u0022:\u0022INCERTO ARA\\u00f1A\u0022,\u0022CANTIDAD\u0022:4,\u0022ESTADO\u0022:\u0022\u0022,\u0022scale\u0022:true,\u0022tipo\u0022:\u0022base\u0022},{\u0022DETALLE\u0022:\u0022BISAGRA\u0022,\u0022CANTIDAD\u0022:2,\u0022ESTADO\u0022:\u0022\u0022,\u0022scale\u0022:true,\u0022tipo\u0022:\u0022base\u0022},{\u0022DETALLE\u0022:\u0022CERRADURA\u0022,\u0022CANTIDAD\u0022:1,\u0022ESTADO\u0022:\u0022\u0022,\u0022scale\u0022:true,\u0022tipo\u0022:\u0022base\u0022},{\u0022DETALLE\u0022:\u0022TIRADOR\u0022,\u0022CANTIDAD\u0022:1,\u0022ESTADO\u0022:\u0022\u0022,\u0022scale\u0022:true,\u0022tipo\u0022:\u0022base\u0022},{\u0022DETALLE\u0022:\u0022TORNILLOS DE 40MM\u0022,\u0022CANTIDAD\u0022:4,\u0022ESTADO\u0022:\u0022\u0022,\u0022scale\u0022:true,\u0022tipo\u0022:\u0022base\u0022},{\u0022DETALLE\u0022:\u0022ESCUADRA PL\\u00e1STICA\u0022,\u0022CANTIDAD\u0022:1,\u0022ESTADO\u0022:\u0022\u0022,\u0022scale\u0022:true,\u0022tipo\u0022:\u0022base\u0022},{\u0022DETALLE\u0022:\u0022TORNILLO 4,5 X 20\u0022,\u0022CANTIDAD\u0022:8,\u0022ESTADO\u0022:\u0022\u0022,\u0022scale\u0022:true,\u0022tipo\u0022:\u0022derivado\u0022},{\u0022DETALLE\u0022:\u0022TORNILLO 3,5 X 20\u0022,\u0022CANTIDAD\u0022:4,\u0022ESTADO\u0022:\u0022\u0022,\u0022scale\u0022:true,\u0022tipo\u0022:\u0022derivado\u0022},{\u0022DETALLE\u0022:\u0022TORNILLO 3,5 X 15\u0022,\u0022CANTIDAD\u0022:2,\u0022ESTADO\u0022:\u0022\u0022,\u0022scale\u0022:true,\u0022tipo\u0022:\u0022derivado\u0022}],\u0022encontrado\u0022:true}]'),

      addRow(i){ this.productos[i].rows.push({ DETALLE:'', CANTIDAD:0, ESTADO:'', scale:false }) },
      removeRow(i, j){ this.productos[i].rows.splice(j, 1) },

      // ===== Helpers =====
    
   csrf(){
  const m = document.querySelector('meta[name=csrf-token]');
  return m ? m.getAttribute('content') : 'dFMje70LKD6bTJIwXCQZo08E6f6g6mmfHLjcvm9N';
},
buildPayload(){
  return {
    // encabezado que espera el service
    pedido: this.pedido || null,
    ordenCompra: this.ordenCompra || null, // <-- ¡camelCase!
    proyecto: this.proyecto || null,
    // lista de productos que espera prepararDatosDesdeDemo()
    productos: this.productos.map(p => ({
      // el service arma todo a partir de estas claves
      label: p.label,
      muebles: Number(p.muebles) || 1,
      dim: p.dim,
      dimBuscadas: p.dimBuscadas,
      con_cubierta: !!p.con_cubierta,
      encontrado: !!p.encontrado,
      // rows -> items (lo hace el service)
      rows: (p.rows || []).map(r => ({
        DETALLE: r.DETALLE || '',
        CANTIDAD: Number(r.CANTIDAD) || 0,
        ESTADO: r.ESTADO || '',
        tipo: r.tipo || 'base',   // preserva 'base'/'derivado'
        scale: !!r.scale
      }))
    })),
  };
},
async save(){
  try{
    const ok = await Swal.fire({
      icon:'question', title:'¿Guardar ficha?', text:'Se guardará la ficha (múltiple).',
      showCancelButton:true, confirmButtonText:'Sí, guardar', cancelButtonText:'Cancelar',
      confirmButtonColor:'#2563eb'
    });
    if(!ok.isConfirmed) return;

    const res = await fetch('http://127.0.0.1:8000/herrajes/gabinete/guardar', {
      method:'POST',
      headers:{
        'Content-Type':'application/json',
        'X-CSRF-TOKEN': this.csrf(),
        'Accept':'application/json',
      },
      body: JSON.stringify(this.buildPayload())
    });

    if(!res.ok){
      const err = await res.json().catch(()=>({message:'Error'}));
      throw new Error(err.message || 'Error al guardar');
    }
    const out = await res.json();
    Swal.fire({icon:'success', title:'Guardado', text: out.message || 'Ficha guardada correctamente.'});
  }catch(e){
    Swal.fire({icon:'error', title:'Error', text: e.message || 'No se pudo guardar.'});
  }
},
async exportPdf(){
  try{
    const res = await fetch('http://127.0.0.1:8000/herrajes/gabinete/pdf', {
      method:'POST',
      headers:{
        'Content-Type':'application/json',
        'X-CSRF-TOKEN': this.csrf(),
        'Accept':'application/pdf',
      },
      body: JSON.stringify(this.buildPayload())
    });
    if(!res.ok){
      const errTxt = await res.text();
      throw new Error('No se pudo generar el PDF: ' + errTxt);
    }
    const blob = await res.blob();
    const url = URL.createObjectURL(blob);
    const a = document.createElement('a');
    const nice = (this.proyecto || 'ficha_herrajes') + '_' + (new Date().toISOString().slice(0,19).replace(/[:T]/g,'-')) + '.pdf';
    a.href = url; a.download = nice;
    document.body.appendChild(a); a.click();
    a.remove(); URL.revokeObjectURL(url);
  }catch(e){
    Swal.fire({icon:'error', title:'Error', text: e.message || 'No se pudo descargar el PDF.'});
  }
},

      async fakeExcel(){ Swal.fire({icon:'info', title:'Exportar Excel', text:'Acción simulada. Luego conectamos la ruta.'}); },
